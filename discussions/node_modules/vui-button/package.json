    "name": "D2L Corporation"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "vui-gradient": "^0.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "csslint": "^0.10.0",
    "jasmine-dom-matchers": "^0.1.0",
    "karma": "^0.12.32",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.5",
    "karma-cli": "0.0.4",
    "less": "^2.5.1",
    "node-sass": "^3.1.1",
    "postcss-cli": "^1.3.1",
